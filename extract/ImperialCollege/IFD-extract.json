{"FAIRSpec.extract.version":"0.0.4-alpha+2202.11.27","keys":[
 {"example":"aggregation v6 provided by Thomas Mies, Imperial College London; curated by Henry Rzepa"},
 {"FAIRSpec.extractor.properties":[
	{"name":"$$ACCESSCODE$$", "source":"./private.properties"}
 ]},
 {"IFD.property.collectionset.source_data_license_uri":"https://creativecommons.org/licenses/by-nc/4.0"},
 {"IFD.property.collectionset.source_data_license_name":"cc-by-nc-4.0"},
 {"IFD.property.collectionset.source_repository_uri":"https://data.hpc.imperial.ac.uk/resolve/?doi=10386&access={$$ACCESSCODE$$}"}, 
 
 {"##":"todo -- allow for remote API, not just remote file (as in FigShare)"},
 {"remote-data":"{IFD.property.collectionset.source_data_uri::https://data.hpc.imperial.ac.uk/resolve/?doi=11652&file=10&access={$$ACCESSCODE$$}"},

 {"##":"local path will be overridden in ExtractorTest, and it does not actually exist"},
 {"data":"{IFD.property.collectionset.source_data_uri::}"},
 {"path":"{data}|"},
 
 {"##":"ignore for now"},
 {"FAIRSpec.extractor.ignore": "document"},
 
 {"##":"use identifier strings rather than array-index numbers in the finding aid's structure-data association collection"},
 {"FAIRSpec.extractor.flag.association.byid":"true"},
 {"FAIRSpec.extractor.metadata.asso.structuredata":""},

 {"FAIRSpec.extractor.flags" : ""},

 {"##":"METADATA is a special key that allows generation of IFDParameter metadata for any defined object"},

 {"METADATA":[
	{"FOR":"compound","METADATA_FILE":"./Manifest.xlsx","METADATA_KEY":"TM compound number"}
 ]},

 {"##":"for each ZipEntry...."},
 
 {"##":"trigger a new association if the id changes"},
 {"assoc":"{compound=IFD.property.association.structuredata.id::*}"},

 {"##":"not implemented...FAIRSpec.extractor.assign __ {IFD.property.association.structuredata.label::Compound <compound>}"},

 {"##":"add structure objects for top-level files or files within a structures/[id]/ directory or a CIF file"},
 {"#<ignoring lower-level cdxml>#FAIRSpec.extractor.object":"{path}{IFD.representation.structure.cdxml::{assoc}/{IFD.property.structure.id::<compound>}.cdxml}"},

 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.cdxml::{assoc}/**/{IFD.property.structure.id::<compound>}.cdxml}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.png::{assoc}/{IFD.property.structure.id::<compound>}.png}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.mol_2d::{assoc}/{IFD.property.structure.id::<compound>}.mol}"}, 
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.mol_2d::{assoc}/{IFD.property.structure.id::<compound>}-v3.mol}"}, 

 {"##":"Lots of case issues with cif data!"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.cif::{assoc}/{IFD.property.structure.id::<compound>}-{regex::(?i)}X-Ray{regex::(?-i)}/*.cif}"},

 {"##":"TODO structures/ directory option FAIRSpec.extractor.object_:_{path}{IFD.representation.structure.cdxml::{assoc}/structures/{IFD.property.structure.id::*}/*.cdxml}"},

 {"##":"add data objects and their representations for lower-level files"},

 {"##":"Computional"},

 {"FAIRSpec.extractor.object":"{path}{IFD.representation.structure.cml::{assoc}/{IFD.property.structure.id::<compound>-comp/*}/*.cml}"},

 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.comp.input::{assoc}/{IFD.property.dataobject.id::<compound>-comp/*}/*.gjf}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.comp.output::{assoc}/{IFD.property.dataobject.id::<compound>-comp/*}/*.fchk}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.comp.output::{assoc}/{IFD.property.dataobject.id::<compound>-comp/*}/*.log}"},

 {"##":"IR"},

 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.ir.document::{assoc}/{IFD.property.dataobject.id::*-IR}/*.pdf}"},
 
 {"##":"NMR"},
 
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.nmr.ventor_dataset::{assoc}/**/{IFD.property.dataobject.id::*.mnova}}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.nmr.ventor_dataset::{assoc}/**/{IFD.property.dataobject.id::*}.jdx}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.nmr.ventor_dataset::{assoc}/**/{IFD.property.dataobject.id::*}.zip}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.nmr.ventor_dataset::{assoc}/*/{IFD.property.dataobject.id::*}.pdf}"},
 {"FAIRSpec.extractor.object":"{path}{IFD.representation.dataobject.fairspec.nmr.ventor_dataset::{assoc}/*/*/{IFD.property.dataobject.id::*}.pdf}"},
 
 {"##":"...next"} 
]}
 